{
  "name": "codefarmert",
  "scripts": {
      "postdeploy": "([ $STAGING_DATABASE_URL ] && (pg_dump $STAGING_DATABASE_URL | psql $DATABASE_URL)) && python manage.py migrate"
  },
  "env": {
    "STAGING_DATABASE_URL": {
      "required": true
    },
    "ALLOWED_HOSTS": {
      "required": true
    },
    "AWS_S3_ACCESS_KEY_ID": {
      "required": true
    },
    "AWS_S3_SECRET_ACCESS_KEY": {
      "required": true
    },
    "AWS_STORAGE_BUCKET_NAME": {
      "required": true
    },
    "DJANGO_SETTINGS_MODULE": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true
    }
  },
  "formation": {
  },
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ]
}
